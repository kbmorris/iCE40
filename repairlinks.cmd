:START
FOR /F "tokens=1,4" %%A IN ('git ls-tree HEAD -r') DO CALL :MAKELINK %%A "%%B" 

:MAKELINK
IF "%1" NEQ "120000" GOTO :EOF

SET LINUXPATH=%2
SET WINDOWSPATH=%LINUXPATH:/=\%
DIR %WINDOWSPATH% | FIND "<SYMLINK>"
IF NOT ERRORLEVEL 1 GOTO :EOF
SET /p LINUXLINK=<%WINDOWSPATH%
SET WINDOWSLINK=%LINUXLINK:/=\%
REM DIR %WINDOWSLINK%
REM IF ERRORLEVEL 1 GOTO :EOF
del %WINDOWSPATH%
mklink %WINDOWSPATH% %WINDOWSLINK%